pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git 'https://github.com/VrushilPatel/HWE.git/'
            }
        }

        stage('Build and Test') {
            steps {
                sh 'npm install'          // Install project dependencies
                sh 'npm run build'        // Build the React project
                sh 'npm test'             // Run tests if applicable
            }
        }
    }
}
